{
  "name": "masum/laravel-json-schema",
  "description": "A lightweight JSON Schema builder for Laravel with fluent API",
  "type": "library",
  "keywords": ["laravel", "json-schema", "validation", "fluent", "api"],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Masum\\JsonSchema\\": "src/"
    },
    "files": [
      "src/Helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Masum\\JsonSchema\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/validation": "^10.0|^11.0",
    "illuminate/console": "^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Masum\\JsonSchema\\SchemaServiceProvider"
      ],
      "aliases": {
        "JsonSchema": "Masum\\JsonSchema\\Facades\\Schema"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}